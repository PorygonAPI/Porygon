<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="br.com.porygon.PopUpController" spacing="10" alignment="CENTER">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>
    <Button fx:id="btnExcluir" text="Excluir"/>
    <Button fx:id="btnEditar" text="Editar"/>
    <Button fx:id="btnManter" text="Manter"/>
    <TextField fx:id="textField" promptText="Digite o novo valor" visible="false"/>
    <Button fx:id="btnSalvar" text="Salvar" visible="false"/>
</VBox>